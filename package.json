{
  "name": "atacado-celular",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "npm run build && npm run start & sleep 10 && lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html --chrome-flags='--headless' && kill %1",
    "perf": "npm run build && npm run start & sleep 5 && node scripts/performance-audit.js && kill %1",
    "bundle-stats": "npx next-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "build:prod": "npm run type-check && npm run lint && npm run build",
    "analyze:custom": "node scripts/analyze-bundle.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.534.0",
    "multer": "^2.0.2",
    "next": "^15.0.3",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.0.3",
    "@svgr/webpack": "^8.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "^15.0.3",
    "lighthouse": "^12.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "web-vitals": "^4.2.4"
  }
}
